              "view_count": 71,
              "identifier": "potter_stewart",
              "name": "Potter Stewart"
            },
            "byte_stop": 3056619,
            "stop": 738.279,
            "byte_start": 3055783,
            "text_blocks": [
              {
                "text": "Right.",
                "start": 738.078,
                "byte_stop": 3056619,
                "byte_start": 3055783,
                "stop": 738.279
              }
            ]
          },
          {
            "start": 738.279,
            "speaker": {
              "last_name": "Furry",
              "ID": 42677,
              "roles": null,
              "length_of_service": 0,
              "thumbnail": null,
              "href": "https://api.oyez.org/people/thelma_c_furry",
