          },
          {
            "byte_start": 3127108,
            "stop": 775.348,
            "byte_stop": 3130138,
            "text_blocks": [
              {
                "text": "Yes sir.",
                "start": 774.598,
                "byte_start": 3127108,
                "stop": 775.348,
                "byte_stop": 3130138
              }
            ],
            "start": 774.598,
            "speaker": {
