            }
          },
          {
            "byte_start": 3071206,
            "stop": 771.867,
            "byte_stop": 3116241,
            "text_blocks": [
              {
                "text": "It is clear from the language and the legislative history of that amendment that it was directed -- pinpointed indeed at federal elections only.",
                "start": 760.625,
                "byte_start": 3071206,
                "stop": 771.867,
                "byte_stop": 3116241
              }
            ],
            "start": 760.625,
            "speaker": {
              "identifier": "joseph_c_carter_jr",
              "view_count": 0,
              "length_of_service": 0,
              "roles": null,
