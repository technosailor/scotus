{
  "title": "Oral Argument - October 19, 1955",
  "damaged": null,
  "public_note": null,
  "transcript": {
    "sections": [
      {
        "start": 0,
        "byte_stop": 3111957,
        "byte_start": 28672,
        "turns": [
          {
            "start": 0,
