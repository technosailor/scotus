            },
            "byte_stop": 1671043,
            "stop": 410.588,
            "byte_start": 1670416,
            "text_blocks": [
              {
                "text": "Yes, sir.",
                "start": 410.417,
                "byte_stop": 1671043,
                "byte_start": 1670416,
                "stop": 410.588
              }
            ]
          },
          {
            "start": 410.588,
