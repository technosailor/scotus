            "byte_start": 1663101,
            "text_blocks": [
              {
                "text": "About automatically.",
                "start": 408.596,
                "byte_stop": 1670416,
                "byte_start": 1663101,
                "stop": 410.417
              }
            ]
          },
          {
            "start": 410.417,
            "speaker": {
