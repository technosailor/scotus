            "byte_start": 1609707,
            "text_blocks": [
              {
                "text": "(Inaudible)",
                "start": 395.234,
                "byte_stop": 1619843,
                "byte_start": 1609707,
                "stop": 397.771
              }
            ]
          },
          {
            "start": 397.771,
            "speaker": {
              "last_name": "Agrin",
              "ID": 24881,
              "roles": null,
              "length_of_service": 0,
              "thumbnail": null,
              "href": "https://api.oyez.org/people/gloria_agrin",
