                  "id": 2694,
                  "date_end": -231793200,
                  "date_start": -975780000,
                  "institution_name": "Supreme Court of the United States"
                }
              ],
              "length_of_service": 8611,
              "thumbnail": {
                "size": 45083,
                "id": 32706,
                "href": "https://api.oyez.org/sites/default/files/images/people/felix_frankfurter/felix_frankfurter.thumb.png",
                "mime": "image/png"
              },
              "href": "https://api.oyez.org/people/felix_frankfurter",
              "view_count": 0,
              "identifier": "felix_frankfurter",
              "name": "Felix Frankfurter"
            },
            "byte_stop": 1573240,
            "stop": 386.131,
            "byte_start": 1560283,
            "text_blocks": [
              {
                "text": "Have they been out on bail at the time the order was served?",
                "start": 382.892,
                "byte_stop": 1573240,
                "byte_start": 1560283,
                "stop": 386.131
              }
            ]
          },
          {
            "start": 386.131,
            "speaker": {
              "last_name": "Agrin",
              "ID": 24881,
              "roles": null,
              "length_of_service": 0,
              "thumbnail": null,
              "href": "https://api.oyez.org/people/gloria_agrin",
              "view_count": 0,
              "identifier": "gloria_agrin",
              "name": "Gloria Agrin"
            },
            "byte_stop": 1609707,
            "stop": 395.234,
            "byte_start": 1573240,
            "text_blocks": [
              {
                "text": "Some of them had and some of them had not.",
                "start": 386.131,
                "byte_stop": 1592048,
                "byte_start": 1573240,
                "stop": 390.843
              },
              {
                "text": "For instance, Mr. Sklar had been in jail just prior to the issuance of this order.",
                "start": 390.843,
                "byte_stop": 1609707,
                "byte_start": 1592048,
                "stop": 395.234
              }
            ]
          },
          {
            "start": 395.234,
            "speaker": {
              "last_name": "Frankfurter",
              "ID": 15050,
              "roles": [
                {
                  "role_title": "Associate Justice of the Supreme Court of the United States",
                  "appointing_president": "Franklin D. Roosevelt",
                  "type": "scotus_justice",
                  "href": "https://api.oyez.org/preson_role/scotus_justice/2694",
                  "id": 2694,
                  "date_end": -231793200,
                  "date_start": -975780000,
                  "institution_name": "Supreme Court of the United States"
                }
              ],
              "length_of_service": 8611,
              "thumbnail": {
