            },
            "byte_stop": 1703016,
            "stop": 418.576,
            "byte_start": 1681596,
            "text_blocks": [
              {
                "text": "That is correct.",
                "start": 413.222,
                "byte_stop": 1687552,
                "byte_start": 1681596,
                "stop": 414.707
              },
              {
                "text": "There is Gannet, Bittleman, and Jones --",
                "start": 414.707,
                "byte_stop": 1703016,
                "byte_start": 1687552,
                "stop": 418.576
              }
            ]
          },
          {
            "start": 418.576,
            "speaker": null,
            "byte_stop": 1714719,
            "stop": 421.502,
            "byte_start": 1703016,
            "text_blocks": [
              {
                "text": "(Inaudible)",
                "start": 418.576,
                "byte_stop": 1714719,
                "byte_start": 1703016,
                "stop": 421.502
              }
            ]
          },
          {
            "start": 421.502,
            "speaker": {
              "last_name": "Agrin",
              "ID": 24881,
              "roles": null,
              "length_of_service": 0,
              "thumbnail": null,
              "href": "https://api.oyez.org/people/gloria_agrin",
