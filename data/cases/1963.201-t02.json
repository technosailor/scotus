            },
            "byte_stop": 3365700,
            "stop": 815.566,
            "byte_start": 3357132,
            "text_blocks": [
              {
                "text": "But he was not a certificated carrier --",
                "start": 813.412,
                "byte_stop": 3365700,
                "byte_start": 3357132,
                "stop": 815.566
              }
            ]
          },
          {
            "start": 815.566,
            "speaker": {
              "last_name": "Layne",
              "ID": 16466,
              "roles": null,
              "length_of_service": 0,
              "thumbnail": null,
              "href": "https://api.oyez.org/people/a_alvis_layne",
              "view_count": 0,
              "identifier": "a_alvis_layne",
              "name": "A. Alvis Layne"
            },
            "byte_stop": 3367894,
            "stop": 816.099,
            "byte_start": 3365700,
            "text_blocks": [
              {
                "text": "Of course --",
                "start": 815.566,
                "byte_stop": 3367894,
                "byte_start": 3365700,
                "stop": 816.099
              }
            ]
          },
          {
            "start": 816.099,
            "speaker": {
              "last_name": "White",
              "ID": 15071,
              "roles": [
                {
                  "role_title": "Associate Justice of the Supreme Court of the United States",
                  "appointing_president": "John F. Kennedy",
                  "type": "scotus_justice",
                  "href": "https://api.oyez.org/preson_role/scotus_justice/2715",
                  "id": 2715,
                  "date_end": 741243600,
                  "date_start": -243367200,
                  "institution_name": "Supreme Court of the United States"
