            },
            "byte_stop": 447404,
            "stop": 85.982,
            "byte_start": 439463,
            "text_blocks": [
              {
                "text": "The -- the complete record is here.",
                "start": 83.992,
                "byte_stop": 447404,
                "byte_start": 439463,
                "stop": 85.982
              }
            ]
          },
          {
            "start": 85.982,
            "speaker": {
              "last_name": "Dorn",
              "ID": 15850,
              "roles": null,
              "length_of_service": 0,
