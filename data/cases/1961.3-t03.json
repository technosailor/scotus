              "roles": null,
              "href": "https://api.oyez.org/people/richard_schifter"
            },
            "byte_start": 1577671,
            "start": 368.558,
            "stop": 371.348,
            "text_blocks": [
              {
                "byte_start": 1577671,
                "start": 368.558,
                "byte_stop": 1588851,
                "text": "Which statute, Your Honor, the 1891?",
                "stop": 371.348
              }
            ]
          },
