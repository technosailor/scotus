            "text_blocks": [
              {
                "byte_start": 1791979,
                "start": 422.122,
                "byte_stop": 1825729,
                "text": "I assume that there was a special interest at that time in attracting stable people to settle in the general area.",
                "stop": 430.551
              },
              {
                "byte_start": 1825729,
                "start": 430.551,
                "byte_stop": 1832521,
                "text": "I'm going to assume that this was --",
