            "text_blocks": [
              {
                "byte_start": 1604838,
                "start": 375.328,
                "byte_stop": 1609435,
                "text": "Only these three?",
                "stop": 376.49
              },
              {
                "byte_start": 1609435,
                "start": 376.49,
                "byte_stop": 1615078,
                "text": "Are there any other?",
                "stop": 377.895
              }
            ]
          },
          {
            "byte_stop": 1767633,
            "speaker": {
              "view_count": 0,
              "length_of_service": 0,
              "thumbnail": null,
              "ID": 40513,
              "identifier": "richard_schifter",
