            "byte_start": 2404707,
            "stop": 578.796,
            "byte_stop": 2418709,
            "text_blocks": [
              {
                "text": "Precisely.",
                "start": 575.307,
                "byte_start": 2404707,
                "stop": 578.252,
                "byte_stop": 2416515
              },
              {
                "text": "I believe that's all I have.",
                "start": 578.252,
                "byte_start": 2416515,
                "stop": 578.796,
                "byte_stop": 2418709
              }
            ],
            "start": 575.307,
            "speaker": {
              "identifier": "j_mitchell_reese_jr",
              "view_count": 0,
              "length_of_service": 0,
              "roles": null,
