          },
          {
            "byte_start": 1545550,
            "stop": 379.916,
            "byte_stop": 1548372,
            "text_blocks": [
              {
                "text": "And this one did.",
                "start": 379.217,
                "byte_start": 1545550,
                "stop": 379.916,
                "byte_stop": 1548372
              }
            ],
            "start": 379.217,
            "speaker": {
              "identifier": "charles_e_whittaker",
              "view_count": 0,
              "length_of_service": 1832,
              "roles": [
                {
                  "institution_name": "Supreme Court of the United States",
                  "type": "scotus_justice",
                  "appointing_president": "Dwight D. Eisenhower",
                  "date_end": -244749600,
                  "date_start": -403034400,
                  "id": 2726,
                  "href": "https://api.oyez.org/preson_role/scotus_justice/2726",
                  "role_title": "Associate Justice of the Supreme Court of the United States"
                }
              ],
              "name": "Charles E. Whittaker",
              "ID": 15082,
              "last_name": "Whittaker",
              "href": "https://api.oyez.org/people/charles_e_whittaker",
              "thumbnail": {
