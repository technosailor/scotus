            "byte_start": 1772335,
            "byte_stop": 1787277,
            "stop": 420.96,
            "text_blocks": [
              {
                "stop": 420.96,
                "text": "Is that on the record or is it de novo?",
                "byte_stop": 1787277,
                "byte_start": 1772335,
                "start": 417.217
              }
            ],
            "start": 417.217
          },
          {
            "speaker": {
              "roles": null,
              "view_count": 0,
              "href": "https://api.oyez.org/people/richard_a_billups_jr",
              "name": "Richard A. Billups, Jr.",
