                "id": 32699,
                "size": 55559
              },
              "ID": 15071,
              "identifier": "byron_r_white"
            },
            "byte_start": 1366810,
            "byte_stop": 1376110,
            "stop": 318.154,
            "text_blocks": [
              {
                "stop": 318.154,
                "text": "This a case (Inaudible)",
                "byte_stop": 1376110,
                "byte_start": 1366810,
                "start": 315.841
              }
            ],
            "start": 315.841
          },
          {
            "speaker": {
              "roles": null,
              "view_count": 0,
              "href": "https://api.oyez.org/people/richard_a_billups_jr",
              "name": "Richard A. Billups, Jr.",
              "last_name": "Billups,",
              "length_of_service": 0,
