          {
            "byte_start": 28672,
            "stop": 30.499,
            "byte_stop": 150716,
            "text_blocks": [
              {
                "text": "Number 189, Milton Knapp versus Mitchell D. Schweitzer et al.",
                "start": 0,
                "byte_start": 28672,
                "stop": 18.199,
                "byte_stop": 101501
              },
              {
                "text": "I -- I beg your pardon.",
                "start": 18.199,
                "byte_start": 101501,
                "stop": 19.155,
                "byte_stop": 105368
              },
              {
                "text": "Yes, I beg your pardon.",
                "start": 19.155,
                "byte_start": 105368,
                "stop": 20.177,
                "byte_stop": 109443
              },
              {
                "text": "I -- I skipped.",
                "start": 20.177,
                "byte_start": 109443,
                "stop": 22.136,
                "byte_stop": 117279
              },
              {
                "text": "Once again, Number 104, Parmelee Transportation Company versus Atchison, Topeka and Santa Fe Railway.",
                "start": 22.136,
                "byte_start": 117279,
                "stop": 30.384,
                "byte_stop": 150298
              },
              {
                "text": "I beg your --",
                "start": 30.384,
                "byte_start": 150298,
                "stop": 30.499,
                "byte_stop": 150716
              }
            ],
            "start": 0,
            "speaker": {
