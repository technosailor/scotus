            },
            "byte_stop": 542908,
            "stop": 109.865,
            "byte_start": 532041,
            "text_blocks": [
              {
                "text": "It does, sir, it does.",
                "start": 107.151,
                "byte_stop": 542386,
                "byte_start": 532041,
                "stop": 109.721
              },
              {
                "text": "It --",
                "start": 109.721,
                "byte_stop": 542908,
                "byte_start": 542386,
                "stop": 109.865
              }
            ]
          },
          {
            "start": 109.865,
            "speaker": {
              "last_name": "Whittaker",
              "ID": 15082,
              "roles": [
                {
                  "role_title": "Associate Justice of the Supreme Court of the United States",
                  "appointing_president": "Dwight D. Eisenhower",
                  "type": "scotus_justice",
                  "href": "https://api.oyez.org/preson_role/scotus_justice/2726",
                  "id": 2726,
                  "date_end": -244749600,
                  "date_start": -403034400,
                  "institution_name": "Supreme Court of the United States"
                }
              ],
              "length_of_service": 1832,
              "thumbnail": {
                "size": 50854,
                "id": 32700,
                "href": "https://api.oyez.org/sites/default/files/images/people/charles_e_whittaker/charles_e_whittaker.thumb.png",
                "mime": "image/png"
